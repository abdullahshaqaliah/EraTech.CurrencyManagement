@using Microsoft.AspNetCore.Mvc.Localization
@model EraTech.CurrencyManagement.Web.Pages.SettingManagement.Components.CurrencySettingGroup.CurrencySettingGroupViewComponent.UpdateCurrencySettingsViewModel
@using Volo.Abp.SettingManagement.Localization
@inject IHtmlLocalizer<AbpSettingManagementResource> L
    <abp-dynamic-form abp-model="@Model" data-method="PUT" class="form" data-request-type="json" id="FacebookWhatsAppSettingsForm" action="/api/setting-management/Currency">
        <div class="card-body border-top p-9">
            <abp-form-content class="row" />
        </div>
        <div class="card-footer d-flex justify-content-end py-6 px-9">
            <button type="submit" class="btn btn-primary me-2"><i class="fas fa-save"></i> @L["Save"]</button>

        </div>
    </abp-dynamic-form>